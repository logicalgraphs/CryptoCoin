#!/usr/bin/env expect

log_user 0
spawn ghci
log_user 1

expect ".*> "
send ":set prompt \"ghci> \"\n"

expect "ghci> "
send ":set -i$::env(CRYPTOCOIN_DIR)/src:$::env(GHC_HOME)\n"
expect "ghci> "
send ":load src/CryptoCoin/CoinMarketCap/Go.hs\n"
expect "ghci> "
send "go\n"
expect "ghci> "
send ":q\n"

interact
